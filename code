function draw() {

	renderer.render(scene, camera);
  var num1 = mouseTracer();
  

  move01Mesh.position.x += num1*0.4;
  move02Mesh.position.x -= num1*0.5;
  move03Mesh.position.x += num1*0.4;
  move04Mesh.position.x -= num1*0.3;
  ball03Mesh.position.y += num1*0.12;
