  function distance(ax,ay,bx,by) {
  var x1 = bx - ax;
  var y1 = by - ay;
  var c1 = Math.sqrt(x1 * x1 + y1 * y1);
  return c1;
}  
  
  
  function mouseTracer(){

//   let velocity = 3.0+mouseY*0.03;
let  velocity = 1.0;
  x =  (mouseX ) * 0.2 ;
  y =  (mouseY) * 0.1 ;
//   let distanceNum = -distance(x,y,mouseX,mouseY)*0.00001;
  
let distanceNum = y*0.001;
// console.log(distanceNum);//You can see the object is moving closer and closer to the mouse position  
    return distanceNum;
}
      
      
